{
  "name": "@fedevlab/eslint-config",
  "version": "1.0.0",
  "description": "Recommand ESLint config from @fedevlab",
  "keywords": [
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/suiyi8760/ux-dev-toolkit.git"
  },
  "license": "MIT",
  "author": "Johnny <johnnyxuto17@gmail.com>",
  "files": [
    "index.js",
    "vue-ts.js",
    "react.js",
    "solid.js",
    "ts.js",
    "shared"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "release": "lerna version --conventional-commits ",
    "test": "pnpm run lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "cross-env-shell commitlint -e $GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ]
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^8.0.1",
    "husky": "^3.0.9",
    "prettier": "^2.8.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "eslint": "^8.0.1",
    "typescript": "^4"
  }
}
